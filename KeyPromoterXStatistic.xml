<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘Button1 Click' or '⌃⌥⌘→'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Errors In Solution">
        <Item>
          <IdeaActionID>ActivateErrorsInSolutionToolWindow</IdeaActionID>
          <ShortCut>'⌃⌘2'</ShortCut>
          <Description>Errors In Solution</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Event Log">
        <Item>
          <IdeaActionID>ActivateEventLogToolWindow</IdeaActionID>
          <ShortCut>'⌘9'</ShortCut>
          <Description>Event Log</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="NuGet">
        <Item>
          <IdeaActionID>ActivateNuGetToolWindow</IdeaActionID>
          <ShortCut>'⌘7' or '⇧⌘7'</ShortCut>
          <Description>NuGet</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥§'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>