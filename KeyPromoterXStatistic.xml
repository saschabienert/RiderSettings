<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Errors In Solution">
        <Item>
          <IdeaActionID>ActivateErrorsInSolutionToolWindow</IdeaActionID>
          <ShortCut>'⌃⌘2'</ShortCut>
          <Description>Errors In Solution</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="NuGet">
        <Item>
          <IdeaActionID>ActivateNuGetToolWindow</IdeaActionID>
          <ShortCut>'⌘7' or '⇧⌘7'</ShortCut>
          <Description>NuGet</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥§'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>